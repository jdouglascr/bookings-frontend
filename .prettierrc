{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 140,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "src/**/*.html",
      "options": {
        "parser": "angular",
        "printWidth": 120
      }
    }
  ]
}
